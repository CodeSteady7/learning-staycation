<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
	<!-- DataTales Example -->
	<div class="row">
		<% for(let a = 0; a < item.imageId.length; a++) { %>
		<div class="col-md-4">
			<div class="card shadow mb-4 mt-2">
				<div class="card-body">
					<img src="https://admin-staycation.herokuapp.com/<%= item.imageId[a].imageUrl %>" alt="image" style="width: 100%; display: block;" />
				</div>
			</div>
		</div>
		<% } %>
	</div>
	<a href="/admin/item" class="btn btn-warning">Back</a>
</div>
